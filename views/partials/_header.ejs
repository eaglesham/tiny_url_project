<h1>Tiny URL Project</h1>
<table>

  <% if (!username){ %>
  <tr><td>
    <form action="/login" method="POST">
      <input id="login" type="text" name="username" placeholder="enter username" style="width: 300px">
      <input type="submit" value="submit">
    </form>
  </td></tr>
  <% } else{ %>
  <tr><td>
    <%= username %>
  </td></tr>
  <tr><td>
    <form action="/logout" method="POST">
      <input form="logout" type="submit" value="logout">
    </form>
  </td></tr>
  <% } %>

  </table>



